{
  "name": "svl",
  "version": "5.2.2",
  "description": "Street Vector Layer (SVL)\r The Road layer for the Waze editors, made by editors.",
  "main": "svl.user.js",
  "watch": {
    "compile": "svl.user.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "npx google-closure-compiler --js=svl.user.js --js gcc-externs.js --js_output_file=install.user.js --compilation_level=ADVANCED_OPTIMIZATIONS",
    "compile-pp": "npx google-closure-compiler --formatting=pretty_print --js=svl.user.js --js gcc-externs.js --js_output_file=install.user.js --compilation_level=ADVANCED_OPTIMIZATIONS",
    "concat": "cross-var concat -o releases\\release-$npm_package_version.user.js header.js install.user.js",
    "build": "npm run compile && npm run concat",
    "release": "cross-var replace-in-files --regex='\\d+\\.\\d+\\.\\d+' --replacement=$npm_package_version header.js && git add header.js && npm run build",
    "watch": "npm-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bedo2991/svl.git"
  },
  "keywords": [],
  "author": "Francesco Bedini",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bedo2991/svl/issues"
  },
  "homepage": "https://github.com/bedo2991/svl#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/eslint-plugin": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "concat": "^1.0.3",
    "cross-var": "^1.1.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "google-closure-compiler": "^20210505.0.0",
    "npm-watch": "n^0.7.0",
    "replace-in-files-cli": "^1.0.0"
  }
}
